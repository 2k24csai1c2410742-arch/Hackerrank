#include <stdio.h>
#include <stdlib.h>

void sort_array(long long arr[], int n) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                long long temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
}


void solve() {
    int n;
    scanf("%d", &n);

    long long container[n][n];
    long long container_sums[n];
    long long ball_sums[n];

    for (int i = 0; i < n; i++) {
        container_sums[i] = 0;
        ball_sums[i] = 0;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%lld", &container[i][j]);
            container_sums[i] += container[i][j];
            ball_sums[j] += container[i][j];
        }
    }

    sort_array(container_sums, n);
    sort_array(ball_sums, n);

    int possible = 1;
    for (int i = 0; i < n; i++) {
        if (container_sums[i] != ball_sums[i]) {
            possible = 0;
            break;
        }
    }

    if (possible) {
        printf("Possible\n");
    } else {
        printf("Impossible\n");
    }
}

int main() {
    int q;
    scanf("%d", &q);
    while (q--) {
        solve();
    }
    return 0;
}
