#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void solve() {
    int n, k;
    if (scanf("%d %d", &n, &k) != 2) return;

    long long counts[100];
    memset(counts, 0, sizeof(counts));

    long long result = 0;

    counts[0] = 1;

    int current_sum_mod_k = 0;
    int a_i;

    for (int i = 0; i < n; i++) {
        if (scanf("%d", &a_i) != 1) return;

        current_sum_mod_k = (current_sum_mod_k + a_i) % k;

        result += counts[current_sum_mod_k];

        counts[current_sum_mod_k]++;
    }

    printf("%lld\n", result);
}

int main() {
    int t;
    if (scanf("%d", &t) != 1) return 1;

    while (t--) {
        solve();
    }

    return 0;
}
