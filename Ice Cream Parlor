#include <iostream>
#include <vector>

std::vector<int> icecreamParlor(int m, std::vector<int> cost) {
    for (size_t i = 0; i < cost.size(); ++i) {
        for (size_t j = i + 1; j < cost.size(); ++j) {
            if (cost[i] + cost[j] == m) {
                return {static_cast<int>(i + 1), static_cast<int>(j + 1)};
            }
        }
    }
    return {};
}

int main() {
    int t;
    std::cin >> t;
    while (t--) {
        int m, n;
        std::cin >> m >> n;
        std::vector<int> cost(n);
        for (int i = 0; i < n; ++i) {
            std::cin >> cost[i];
        }
        std::vector<int> result = icecreamParlor(m, cost);
        std::cout << result[0] << " " << result[1] << std::endl;
    }
    return 0;
}
