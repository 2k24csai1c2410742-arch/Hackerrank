#include <stdio.h>
#include <string.h>
#include <stdlib.h>

#define MOD 1000000007

long long substrings(char* n) {
    int len = strlen(n);
    long long total = 0;
    long long prev_sum = 0;
    
    // For each position i, we calculate contribution
    // When we move from position i-1 to i:
    // - All previous substrings get extended by current digit
    // - New substrings starting at position i are created
    
    for (int i = 0; i < len; i++) {
        int digit = n[i] - '0';
        
        // prev_sum * 10: all previous substrings extended by one position
        // digit * (i + 1): new substrings - digit appears in (i+1) substrings
        // ending at position i (one for each possible starting position 0..i)
        long long curr_sum = (prev_sum * 10 + (long long)digit * (i + 1)) % MOD;
        
        total = (total + curr_sum) % MOD;
        prev_sum = curr_sum;
    }
    
    return total;
}

int main() {
    char n[200005];
    
    // Read input
    if (scanf("%s", n) != 1) {
        return 1;
    }
    
    // Calculate and print result
    long long result = substrings(n);
    printf("%lld\n", result);
    
    return 0;
}
